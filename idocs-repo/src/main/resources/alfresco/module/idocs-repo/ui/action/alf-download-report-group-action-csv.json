{
  "id": "alf-download-report-group-action-csv",
  "type": "server-group-action",
  "name": {
    "ru": "Скачать Csv-файл",
    "en": "Download Csv-file"
  },
  "config": {
    "id": "download-report-csv-action",
    "params": {
      "reportTitle": "REPORT",
      "actionId": "download-report-csv-action",
      "evaluateBatch": "true",
      "delimiter": ";",
      "columns": "${reportColumns}",
      "batchSize": 100,
      "pageSize": 100,
      "elementsLimit": 1000
    }
  },
  "features": {
    "execForQuery": true,
    "execForRecord": false,
    "execForRecords": true
  }
}
